@model BusJourneyDetailViewModel

@{
    ViewData["Title"] = "OBilet Case Çalışması";
}

<div class="container my-5">
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary position-relative">
        <a class="navbar-brand" href="#"><i class="fas fa-arrow-left"></i></a>
        <div class="journey-header-text">
            <p class="mb-0" style="font-size: 14pt;">@Model.FormHeaderText</p>
            <p class="mb-0" style="font-size: 12pt;">@Model.DepartureDateText</p>
        </div>
    </nav>
    <div class="card">
        <div class="card-body">
            @foreach (var item in Model.BusJourneys)
            {
                <label>@item.OriginLocation</label>
            }
        </div>
    </div>
</div>

@section scripts{
    <script>
            $(document).ready(function () {

              
            });
    </script>

}
